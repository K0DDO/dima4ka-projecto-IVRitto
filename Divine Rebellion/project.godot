config_version=5

[application]

config/name="Divine Rebellion"
run/main_scene="res://Scenes/open_scene.tscn"
config/features=PackedStringArray("4.2", "GL Compatibility")
run/max_fps=120
boot_splash/bg_color=Color(1, 0.917647, 0.941176, 1)
boot_splash/image="res://Art/Tech/boot splash/boot splash.png"
boot_splash/use_filter=false
config/icon="res://Art/Tech/icon.png"
config/windows_native_icon="res://Art/Tech/icon.ico"
boot_splash/minimum_display_time=2000

[autoload]

Global="*res://Scripts/Global.gd"
SaveSettingsData="*res://Scripts/saveSettings.gd"
Brightness="*res://Scenes/brightness.tscn"

[display]

window/size/viewport_width=240
window/size/viewport_height=135
window/size/resizable=false
window/size/window_width_override=1920
window/size/window_height_override=1080
window/stretch/mode="canvas_items"
window/stretch/aspect="keep_width"
window/vsync/vsync_mode=0
mouse_cursor/custom_image="res://Art/Tech/cursor.png"

[filesystem]

import/blender/enabled=false

